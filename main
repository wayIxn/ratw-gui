-- Scripts:

local function QYYEKNC_fake_script() -- RaceAroundTheWorld.Client 
	local script = Instance.new('LocalScript', RaceAroundTheWorld)

	local modules = {
		tweenModule = {
			tween = function(tweenTime, instance, style, direction, contents, rev)
				local ts = game:GetService("TweenService")
				local TI = TweenInfo.new(tweenTime or 1, Enum.EasingStyle[style] or Enum.EasingStyle.Linear, Enum.EasingDirection[direction] or Enum.EasingDirection.InOut, 0, rev, 0)
				ts:Create(instance, TI, contents):Play()
			end
		},
	}
	--variables
	local welcomeFrame = script.Parent.Holder.WelcomeFrame
	
	local thumbType = Enum.ThumbnailType.HeadShot
	local thumbSize = Enum.ThumbnailSize.Size420x420
	local content, isReady = game.Players:GetUserThumbnailAsync(game.Players.LocalPlayer.UserId, thumbType, thumbSize)
	welcomeFrame.Avatar.Image = content
	local components = script.Parent.Holder.Components
	--startup
	task.wait(1)
	script.Parent.Holder.Visible = true
	task.wait(3)
	modules.tweenModule.tween(1, welcomeFrame.Avatar, 'Quint', 'Out', {Size = UDim2.new(0.292, 0, 0.406, 0), Position = UDim2.new(0.5, 0, 0.421, 0)}, false)
	task.wait(1)
	modules.tweenModule.tween(.5, welcomeFrame.WelcomeMessage, 'Quint', 'Out', {TextTransparency = 0, Position = UDim2.new(0.5, 0, 0.708, 0)}, false)
	task.wait(3)
	modules.tweenModule.tween(1, welcomeFrame, 'Quint', 'Out', {Position = UDim2.new(0.5, 0, 2, 0)}, false)
	task.wait(1)
	modules.tweenModule.tween(.5, components.Header.Exit, 'Quint', 'Out', {TextTransparency = 0}, false)
	modules.tweenModule.tween(.5, components.Header.Title, 'Quint', 'Out', {TextTransparency = 0}, false)
	
	components.Header.Exit.MouseButton1Click:Connect(function()
		script.Parent.Holder.Visible = false
		script.Parent:Destroy()
	end)
end
coroutine.wrap(QYYEKNC_fake_script)()
